  /* istanbul ignore next */
  (cov_v8cd4345w().b[1][1]++, obj.__esModule) ?
  /* istanbul ignore next */
  (cov_v8cd4345w().b[0][0]++, obj) :
  /* istanbul ignore next */
  (cov_v8cd4345w().b[0][1]++, {
    default: obj
  });
}
