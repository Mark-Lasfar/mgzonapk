                  children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_form.FormLabel, {
                    children: t('rating')
                  }), /*#__PURE__*/(0, _jsxruntime.jsx)(_form.FormControl, {
                    children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                      className: "flex space-x-1",
                      children: [1, 2, 3, 4, 5].map(star => {
                        /* istanbul ignore next */
                        cov_25x5f3arj().f[7]++;
                        cov_25x5f3arj().s[47]++;
                        return /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Star, {
                          className: `h-6 w-6 cursor-pointer ${star <= field.value ?
                          /* istanbul ignore next */
                          (cov_25x5f3arj().b[10][0]++, 'text-yellow-400 fill-yellow-400') :
                          /* istanbul ignore next */
                          (cov_25x5f3arj().b[10][1]++, 'text-gray-300')}`,
                          onClick: () => {
                            /* istanbul ignore next */
                            cov_25x5f3arj().f[8]++;
                            cov_25x5f3arj().s[48]++;
                            return field.onChange(star);
                          }
                        }, star);
                      })
                    })
                  }), /*#__PURE__*/(0, _jsxruntime.jsx)(_form.FormMessage, {})]
                });
              }
            }), /*#__PURE__*/(0, _jsxruntime.jsx)(_form.FormField, {
              control: form.control,
              name: "comment",
              render: ({
                field
              }) => {
                /* istanbul ignore next */
                cov_25x5f3arj().f[9]++;
                cov_25x5f3arj().s[49]++;
                return /*#__PURE__*/(0, _jsxruntime.jsxs)(_form.FormItem, {
                  children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_form.FormLabel, {
                    children: t('comment')
                  }), /*#__PURE__*/(0, _jsxruntime.jsx)(_form.FormControl, {
                    children: /*#__PURE__*/(0, _jsxruntime.jsx)(_textarea.Textarea, {
                      ...field,
                      placeholder: t('commentPlaceholder')
                    })
                  }), /*#__PURE__*/(0, _jsxruntime.jsx)(_form.FormMessage, {})]
                });
              }
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "flex space-x-2",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
                type: "submit",
                disabled: isSubmitting,
                children: isSubmitting ?
                /* istanbul ignore next */
                (cov_25x5f3arj().b[11][0]++, t('submitting')) :
                /* istanbul ignore next */
                (cov_25x5f3arj().b[11][1]++, t('submit'))
              }), /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
                variant: "outline",
                onClick: () => {
                  /* istanbul ignore next */
                  cov_25x5f3arj().f[10]++;
                  cov_25x5f3arj().s[50]++;
                  return setShowForm(false);
                },
