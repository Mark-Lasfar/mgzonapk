      /* istanbul ignore next */
      (/*#__PURE__*/cov_25x5f3arj().b[6][1]++, (0, _jsxruntime.jsx)("div", {
        className: "space-y-4",
        children: reviews.map((review, index) => {
          /* istanbul ignore next */
          cov_25x5f3arj().f[3]++;
          cov_25x5f3arj().s[43]++;
          return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "border-b pb-4",
            children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "flex items-center space-x-2",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                className: "font-semibold",
                children: review.userName
              }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                className: "flex",
                children: [...Array(5)].map((_, i) => {
                  /* istanbul ignore next */
                  cov_25x5f3arj().f[4]++;
                  cov_25x5f3arj().s[44]++;
                  return /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Star, {
                    className: `h-4 w-4 ${i < review.rating ?
                    /* istanbul ignore next */
                    (cov_25x5f3arj().b[7][0]++, 'text-yellow-400 fill-yellow-400') :
                    /* istanbul ignore next */
                    (cov_25x5f3arj().b[7][1]++, 'text-gray-300')}`
                  }, i);
                })
              })]
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
              className: "text-gray-600 mt-1",
              children: review.comment
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
              className: "text-sm text-gray-500 mt-1",
              children: new Intl.DateTimeFormat(locale, {
                dateStyle: 'medium'
              }).format(new Date(review.createdAt))
            })]
          }, index);
        })
      })),
      /* istanbul ignore next */
      (cov_25x5f3arj().b[8][0]++, status === 'authenticated') &&
      /* istanbul ignore next */
      (cov_25x5f3arj().b[8][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "mt-6",
        children: !showForm ?
        /* istanbul ignore next */
        (/*#__PURE__*/cov_25x5f3arj().b[9][0]++, (0, _jsxruntime.jsx)(_button.Button, {
          onClick: () => {
            /* istanbul ignore next */
            cov_25x5f3arj().f[5]++;
            cov_25x5f3arj().s[45]++;
            return setShowForm(true);
          },
          children: t('writeReview')
        })) :
        /* istanbul ignore next */
        (/*#__PURE__*/cov_25x5f3arj().b[9][1]++, (0, _jsxruntime.jsx)(_form.Form, {
          ...form,
          children: /*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
            onSubmit: form.handleSubmit(onSubmit),
            className: "space-y-4",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_form.FormField, {
              control: form.control,
              name: "rating",
              render: ({
                field
