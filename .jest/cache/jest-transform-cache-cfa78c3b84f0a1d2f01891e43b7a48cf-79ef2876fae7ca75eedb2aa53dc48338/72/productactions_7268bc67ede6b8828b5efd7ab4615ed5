  limit =
  /* istanbul ignore next */
  (cov_1y8lmtzck3().b[133][0]++, 4)
}) {
  /* istanbul ignore next */
  cov_1y8lmtzck3().f[65]++;
  cov_1y8lmtzck3().s[416]++;
  try {
    /* istanbul ignore next */
    cov_1y8lmtzck3().s[417]++;
    await (0, _db.connectToDatabase)();
    const products =
    /* istanbul ignore next */
    (cov_1y8lmtzck3().s[418]++, await _productmodel.default.find({
      _id: {
        $ne: productId
      },
      category,
      isPublished: true,
      status: 'active',
