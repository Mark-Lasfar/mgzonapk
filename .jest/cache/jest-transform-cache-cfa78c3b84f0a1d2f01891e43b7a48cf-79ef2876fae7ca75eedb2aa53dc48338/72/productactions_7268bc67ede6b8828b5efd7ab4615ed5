    /* istanbul ignore next */
    (cov_1y8lmtzck3().s[411]++, await _productmodel.default.findOne({
      slug,
      isPublished: true,
      status: 'active'
    }).lean());
    /* istanbul ignore next */
    cov_1y8lmtzck3().s[412]++;
