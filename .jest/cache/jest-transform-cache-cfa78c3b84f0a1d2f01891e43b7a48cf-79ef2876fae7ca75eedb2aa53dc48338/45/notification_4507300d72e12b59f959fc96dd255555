    });
  } catch (error) {
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[23]++;
    console.error('Firebase admin initialization error:', error);
  }
} else
/* istanbul ignore next */
{
  cov_f3lwgbkyi().b[2][1]++;
}
async function checkEmailRateLimit() {
  /* istanbul ignore next */
  cov_f3lwgbkyi().f[5]++;
  const now =
  /* istanbul ignore next */
  (cov_f3lwgbkyi().s[24]++, Date.now());
  /* istanbul ignore next */
  cov_f3lwgbkyi().s[25]++;
  if (now > emailCounter.resetTime) {
    /* istanbul ignore next */
    cov_f3lwgbkyi().b[3][0]++;
    cov_f3lwgbkyi().s[26]++;
    emailCounter.count = 0;
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[27]++;
    emailCounter.resetTime = now + EMAIL_RATE_LIMIT.windowMs;
  } else
  /* istanbul ignore next */
  {
    cov_f3lwgbkyi().b[3][1]++;
  }
  cov_f3lwgbkyi().s[28]++;
  if (emailCounter.count >= EMAIL_RATE_LIMIT.maxEmails) {
    /* istanbul ignore next */
    cov_f3lwgbkyi().b[4][0]++;
    cov_f3lwgbkyi().s[29]++;
    console.warn('Email rate limit exceeded. Queuing email.');
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[30]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_f3lwgbkyi().b[4][1]++;
  }
  cov_f3lwgbkyi().s[31]++;
  emailCounter.count++;
  /* istanbul ignore next */
  cov_f3lwgbkyi().s[32]++;
  return true;
}
async function sendNotification(options) {
  /* istanbul ignore next */
  cov_f3lwgbkyi().f[6]++;
  cov_f3lwgbkyi().s[33]++;
  try {
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[34]++;
    await (0, _db.connectToDatabase)();
    const {
      userId,
      type,
      title,
      message,
      data =
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[5][0]++, {}),
      channels =
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[6][0]++,
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[7][0]++, _storageconfig.NOTIFICATION_CONFIG.types[type.toUpperCase()]?.defaultChannels) ||
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[7][1]++, ['email'])),
      priority =
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[8][0]++,
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[9][0]++, _storageconfig.NOTIFICATION_CONFIG.types[type.toUpperCase()]?.priority) ||
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[9][1]++, 'medium')),
      expiresAt,
      metadata
    } =
    /* istanbul ignore next */
    (cov_f3lwgbkyi().s[35]++, options);
    // Check seller notification settings
    const seller =
    /* istanbul ignore next */
    (cov_f3lwgbkyi().s[36]++, await _sellermodel.default.findOne({
      userId
    }));
    const notificationSettings =
    /* istanbul ignore next */
    (cov_f3lwgbkyi().s[37]++,
    /* istanbul ignore next */
    (cov_f3lwgbkyi().b[10][0]++, seller?.settings.notifications) ||
    /* istanbul ignore next */
    (cov_f3lwgbkyi().b[10][1]++, {
      email: true,
      sms: false,
      orderUpdates: true,
      marketingEmails: false,
      pointsNotifications: true
    }));
    // Filter channels based on seller settings
    const allowedChannels =
    /* istanbul ignore next */
    (cov_f3lwgbkyi().s[38]++, channels.filter(channel => {
      /* istanbul ignore next */
      cov_f3lwgbkyi().f[7]++;
      cov_f3lwgbkyi().s[39]++;
      if (
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[12][0]++, channel === 'email') &&
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[12][1]++, !notificationSettings.email)) {
        /* istanbul ignore next */
        cov_f3lwgbkyi().b[11][0]++;
        cov_f3lwgbkyi().s[40]++;
        return false;
      } else
      /* istanbul ignore next */
      {
        cov_f3lwgbkyi().b[11][1]++;
      }
      cov_f3lwgbkyi().s[41]++;
      if (
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[14][0]++, channel === 'sms') &&
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[14][1]++, !notificationSettings.sms)) {
        /* istanbul ignore next */
        cov_f3lwgbkyi().b[13][0]++;
        cov_f3lwgbkyi().s[42]++;
        return false;
      } else
      /* istanbul ignore next */
      {
        cov_f3lwgbkyi().b[13][1]++;
      }
      cov_f3lwgbkyi().s[43]++;
      if (channel === 'in_app') {
        /* istanbul ignore next */
        cov_f3lwgbkyi().b[15][0]++;
        cov_f3lwgbkyi().s[44]++;
        return true;
      } else
      /* istanbul ignore next */
      {
        cov_f3lwgbkyi().b[15][1]++;
      }
      cov_f3lwgbkyi().s[45]++;
      if (
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[17][0]++, channel === 'push') &&
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[17][1]++, notificationSettings.pointsNotifications)) {
        /* istanbul ignore next */
        cov_f3lwgbkyi().b[16][0]++;
        cov_f3lwgbkyi().s[46]++;
        return true;
      } else
      /* istanbul ignore next */
      {
        cov_f3lwgbkyi().b[16][1]++;
      }
      cov_f3lwgbkyi().s[47]++;
      return true;
    }));
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[48]++;
    if (allowedChannels.length === 0) {
      /* istanbul ignore next */
      cov_f3lwgbkyi().b[18][0]++;
      cov_f3lwgbkyi().s[49]++;
      return {
        success: true,
        notificationId: null,
        message: 'No allowed channels'
      };
    } else
    /* istanbul ignore next */
    {
      cov_f3lwgbkyi().b[18][1]++;
    }
    const user =
    /* istanbul ignore next */
    (cov_f3lwgbkyi().s[50]++, await _usermodel.default.findById(userId).select('email phone fcmToken locale'));
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[51]++;
    if (!user) {
      /* istanbul ignore next */
      cov_f3lwgbkyi().b[19][0]++;
      cov_f3lwgbkyi().s[52]++;
      throw new Error('User not found');
    } else
    /* istanbul ignore next */
    {
      cov_f3lwgbkyi().b[19][1]++;
    }
    const notification =
    /* istanbul ignore next */
    (cov_f3lwgbkyi().s[53]++, await _notificationmodel.default.create({
      userId,
      type,
      title,
      message,
      data,
      channels: allowedChannels,
      priority,
      expiresAt,
      metadata,
      status: 'pending',
      read: false
    }));
    const promises =
    /* istanbul ignore next */
    (cov_f3lwgbkyi().s[54]++, allowedChannels.map(async channel => {
      /* istanbul ignore next */
      cov_f3lwgbkyi().f[8]++;
      cov_f3lwgbkyi().s[55]++;
      try {
        /* istanbul ignore next */
        cov_f3lwgbkyi().s[56]++;
        switch (channel) {
          case 'email':
            /* istanbul ignore next */
            cov_f3lwgbkyi().b[20][0]++;
            cov_f3lwgbkyi().s[57]++;
            if (
            /* istanbul ignore next */
            (cov_f3lwgbkyi().b[22][0]++, user.email) &&
            /* istanbul ignore next */
            (cov_f3lwgbkyi().b[22][1]++, notificationSettings.email)) {
              /* istanbul ignore next */
              cov_f3lwgbkyi().b[21][0]++;
              const canSendEmail =
              /* istanbul ignore next */
              (cov_f3lwgbkyi().s[58]++, await checkEmailRateLimit());
              /* istanbul ignore next */
              cov_f3lwgbkyi().s[59]++;
              if (!canSendEmail) {
                /* istanbul ignore next */
                cov_f3lwgbkyi().b[23][0]++;
                cov_f3lwgbkyi().s[60]++;
                await _notificationmodel.default.findByIdAndUpdate(notification._id, {
                  status: 'queued',
                  queuedAt: new Date()
                });
                /* istanbul ignore next */
                cov_f3lwgbkyi().s[61]++;
                return;
              } else
              /* istanbul ignore next */
              {
                cov_f3lwgbkyi().b[23][1]++;
              }
              cov_f3lwgbkyi().s[62]++;
              await sendEmail({
                to: user.email,
                subject: title,
                html: getEmailTemplate(type, title, message, data,
                /* istanbul ignore next */
                (cov_f3lwgbkyi().b[24][0]++, user.locale) ||
                /* istanbul ignore next */
                (cov_f3lwgbkyi().b[24][1]++, 'en')),
                data
              });
            } else
            /* istanbul ignore next */
            {
              cov_f3lwgbkyi().b[21][1]++;
            }
            cov_f3lwgbkyi().s[63]++;
            break;
          case 'push':
            /* istanbul ignore next */
            cov_f3lwgbkyi().b[20][1]++;
            cov_f3lwgbkyi().s[64]++;
            if (
            /* istanbul ignore next */
            (cov_f3lwgbkyi().b[26][0]++, user.fcmToken) &&
            /* istanbul ignore next */
