async function addToCart(userId, product, quantity, t) {
  /* istanbul ignore next */
  cov_f3lwgbkyi().f[23]++;
  cov_f3lwgbkyi().s[135]++;
  try {
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[136]++;
    await (0, _db.connectToDatabase)();
    // Validate product and stock
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[137]++;
    if (product.countInStock < quantity) {
      /* istanbul ignore next */
      cov_f3lwgbkyi().b[48][0]++;
      cov_f3lwgbkyi().s[138]++;
      return {
        success: false,
        message: t('insufficientStock')
      };
    } else
    /* istanbul ignore next */
    {
      cov_f3lwgbkyi().b[48][1]++;
    }
    // Simulate adding to cart (assuming a Cart model exists)
    const cartItem =
    /* istanbul ignore next */
    (cov_f3lwgbkyi().s[139]++, {
      productId: product._id,
      name: product.name,
      quantity,
      price: product.pricing.finalPrice
    });
    // Send notification
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[140]++;
    await sendNotification({
      userId,
      type: 'cart_updated',
      title: t('cartUpdatedTitle'),
      message: t('cartUpdatedMessage', {
        productName: product.name
      }),
      channels: ['in_app', 'email'],
      data: {
        productId: product._id,
        quantity
      }
    });
    // Update product stock
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[141]++;
    await Product.findByIdAndUpdate(product._id, {
      $inc: {
        countInStock: -quantity
      }
    });
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[142]++;
    return {
      success: true,
      cartItem
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[143]++;
    console.error('Add to cart error:', error);
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[144]++;
    return {
      success: false,
      message: error instanceof Error ?
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[49][0]++, error.message) :
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[49][1]++, t('addToCartFailed'))
    };
  }
}
