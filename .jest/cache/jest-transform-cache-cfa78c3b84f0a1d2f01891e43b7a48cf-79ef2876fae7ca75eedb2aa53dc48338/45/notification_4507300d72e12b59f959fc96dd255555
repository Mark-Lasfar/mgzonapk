async function sendEmail({
  to,
  subject,
  html,
  data
}) {
  /* istanbul ignore next */
  cov_f3lwgbkyi().f[10]++;
  cov_f3lwgbkyi().s[91]++;
  try {
    const response =
    /* istanbul ignore next */
    (cov_f3lwgbkyi().s[92]++, await resend.emails.send({
      from: `${process.env.SITE_NAME} <${process.env.SENDER_EMAIL}>`,
      to,
      subject,
      html,
      tags: [{
        name: 'category',
        value:
        /* istanbul ignore next */
        (cov_f3lwgbkyi().b[37][0]++, data?.type) ||
        /* istanbul ignore next */
        (cov_f3lwgbkyi().b[37][1]++, 'general')
      }, {
        name: 'priority',
        value:
        /* istanbul ignore next */
        (cov_f3lwgbkyi().b[38][0]++, data?.priority) ||
        /* istanbul ignore next */
        (cov_f3lwgbkyi().b[38][1]++, 'normal')
      }]
    }));
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[93]++;
    if (response.error) {
      /* istanbul ignore next */
      cov_f3lwgbkyi().b[39][0]++;
      cov_f3lwgbkyi().s[94]++;
      throw new Error(response.error.message);
    } else
    /* istanbul ignore next */
    {
      cov_f3lwgbkyi().b[39][1]++;
    }
    cov_f3lwgbkyi().s[95]++;
    return response;
  } catch (error) {
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[96]++;
    console.error('Resend email error:', error);
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[97]++;
    try {
      /* istanbul ignore next */
      cov_f3lwgbkyi().s[98]++;
      await transporter.sendMail({
        from: `"${process.env.SITE_NAME}" <${process.env.GMAIL_USER}>`,
        to,
        subject,
        html
      });
      /* istanbul ignore next */
      cov_f3lwgbkyi().s[99]++;
      return {
        id: 'nodemailer-fallback'
      };
    } catch (nodemailerError) {
      /* istanbul ignore next */
      cov_f3lwgbkyi().s[100]++;
      console.error('Nodemailer error:', nodemailerError);
      /* istanbul ignore next */
      cov_f3lwgbkyi().s[101]++;
      throw new Error('Failed to send email');
    }
  }
}
async function sendPushNotification({
  token,
  title,
  body,
  data
}) {
  /* istanbul ignore next */
  cov_f3lwgbkyi().f[11]++;
  cov_f3lwgbkyi().s[102]++;
  if (!token) {
    /* istanbul ignore next */
    cov_f3lwgbkyi().b[40][0]++;
    cov_f3lwgbkyi().s[103]++;
    return;
  } else
  /* istanbul ignore next */
  {
    cov_f3lwgbkyi().b[40][1]++;
  }
  cov_f3lwgbkyi().s[104]++;
  try {
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[105]++;
    await _firebaseadmin.default.messaging().send({
      token,
      notification: {
        title,
        body
      },
      data: Object.entries(
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[41][0]++, data) ||
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[41][1]++, {})).reduce((acc, [key, value]) => {
        /* istanbul ignore next */
        cov_f3lwgbkyi().f[12]++;
        cov_f3lwgbkyi().s[106]++;
        return {
          ...acc,
          [key]: String(value)
        };
      }, {}),
      android: {
        priority: 'high',
        notification: {
          clickAction: 'FLUTTER_NOTIFICATION_CLICK',
          sound: 'default'
        }
      },
      apns: {
        payload: {
          aps: {
            contentAvailable: true,
            sound: 'default',
            badge: 1
          }
        }
      },
      webpush: {
        headers: {
          Urgency: 'high'
        },
        notification: {
          icon: '/icons/icon-192x192.png',
          badge: '/icons/icon-72x72.png'
        }
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[107]++;
    console.error('Push notification error:', error);
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[108]++;
    throw new Error('Failed to send push notification');
  }
}
