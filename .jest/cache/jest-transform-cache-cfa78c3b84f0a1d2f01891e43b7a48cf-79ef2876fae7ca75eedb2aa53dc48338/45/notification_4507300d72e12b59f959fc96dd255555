              /* istanbul ignore next */
              cov_f3lwgbkyi().b[25][0]++;
              cov_f3lwgbkyi().s[65]++;
              await sendPushNotification({
                token: user.fcmToken,
                title,
                body: message,
                data
              });
            } else
            /* istanbul ignore next */
            {
              cov_f3lwgbkyi().b[25][1]++;
            }
            cov_f3lwgbkyi().s[66]++;
            break;
          case 'sms':
            /* istanbul ignore next */
            cov_f3lwgbkyi().b[20][2]++;
            cov_f3lwgbkyi().s[67]++;
            if (
            /* istanbul ignore next */
            (cov_f3lwgbkyi().b[28][0]++, user.phone) &&
            /* istanbul ignore next */
            (cov_f3lwgbkyi().b[28][1]++, notificationSettings.sms)) {
              /* istanbul ignore next */
              cov_f3lwgbkyi().b[27][0]++;
              cov_f3lwgbkyi().s[68]++;
              await sendSMS({
                to: user.phone,
                message: `${title}: ${message}`
              });
            } else
            /* istanbul ignore next */
            {
              cov_f3lwgbkyi().b[27][1]++;
            }
            cov_f3lwgbkyi().s[69]++;
            break;
          case 'in_app':
            /* istanbul ignore next */
            cov_f3lwgbkyi().b[20][3]++;
            cov_f3lwgbkyi().s[70]++;
            user.notifications =
            /* istanbul ignore next */
            (cov_f3lwgbkyi().b[29][0]++, user.notifications) ||
            /* istanbul ignore next */
            (cov_f3lwgbkyi().b[29][1]++, []);
            /* istanbul ignore next */
            cov_f3lwgbkyi().s[71]++;
            user.notifications.push(notification);
            /* istanbul ignore next */
            cov_f3lwgbkyi().s[72]++;
            await user.save();
            /* istanbul ignore next */
            cov_f3lwgbkyi().s[73]++;
            break;
        }
      } catch (error) {
        /* istanbul ignore next */
        cov_f3lwgbkyi().s[74]++;
        console.error(`${channel} notification error:`, error);
      }
    }));
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[75]++;
    await Promise.allSettled(promises);
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[76]++;
    await notification.markAsSent();
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[77]++;
    return {
      success: true,
      notificationId: notification._id
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[78]++;
    console.error('Notification error:', error);
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[79]++;
    return {
      success: false,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[30][0]++, error.message) :
      /* istanbul ignore next */
      (cov_f3lwgbkyi().b[30][1]++, 'Failed to send notification')
    };
  }
}
