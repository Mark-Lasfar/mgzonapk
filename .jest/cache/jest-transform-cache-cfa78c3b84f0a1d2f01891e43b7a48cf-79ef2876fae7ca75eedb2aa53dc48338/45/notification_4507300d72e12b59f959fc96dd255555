    /* istanbul ignore next */
    {
      cov_f3lwgbkyi().b[45][1]++;
    }
  },
  getUnreadCount: async userId => {
    /* istanbul ignore next */
    cov_f3lwgbkyi().f[18]++;
    cov_f3lwgbkyi().s[125]++;
    await (0, _db.connectToDatabase)();
    /* istanbul ignore next */
    cov_f3lwgbkyi().s[126]++;
    return _notificationmodel.default.countDocuments({
      userId,
      read: false
    });
  },
  getUserNotifications: async (userId, limit =
  /* istanbul ignore next */
  (cov_f3lwgbkyi().b[46][0]++, 10), skip =
  /* istanbul ignore next */
  (cov_f3lwgbkyi().b[47][0]++, 0)) => {
    /* istanbul ignore next */
    cov_f3lwgbkyi().f[19]++;
