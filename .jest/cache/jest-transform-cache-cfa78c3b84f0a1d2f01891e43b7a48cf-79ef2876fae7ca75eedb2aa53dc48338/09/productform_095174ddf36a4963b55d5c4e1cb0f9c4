        /* istanbul ignore next */
        (cov_2difmttrrx().s[77]++, color.sizes.reduce((sum, size) => {
          /* istanbul ignore next */
          cov_2difmttrrx().f[12]++;
          cov_2difmttrrx().s[78]++;
          return sum + size.quantity;
        }, 0));
        /* istanbul ignore next */
        cov_2difmttrrx().s[79]++;
