    const minStocks =
    /* istanbul ignore next */
    (cov_2difmttrrx().s[86]++, warehouseData.map(wh => {
      /* istanbul ignore next */
      cov_2difmttrrx().f[14]++;
      cov_2difmttrrx().s[87]++;
