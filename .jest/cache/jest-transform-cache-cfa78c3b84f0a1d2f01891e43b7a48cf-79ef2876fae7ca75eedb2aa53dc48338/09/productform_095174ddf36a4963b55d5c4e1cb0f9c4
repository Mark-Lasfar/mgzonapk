      children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_form.FormLabel, {
        children: t('colorsAndSizes')
      }), /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
        type: "button",
        variant: "outline",
        size: "sm",
        onClick: addColor,
        children: t('addColor')
      })]
    }), colors.map((color, colorIndex) => {
      /* istanbul ignore next */
      cov_2difmttrrx().f[15]++;
      cov_2difmttrrx().s[92]++;
