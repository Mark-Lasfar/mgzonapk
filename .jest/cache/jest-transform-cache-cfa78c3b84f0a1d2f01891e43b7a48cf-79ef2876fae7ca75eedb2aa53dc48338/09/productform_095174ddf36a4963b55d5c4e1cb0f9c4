      warehouseIndex: selectedWarehouseIndex,
      colorIndex
    });
  };
  /* istanbul ignore next */
  cov_2difmttrrx().s[73]++;
  const updateTotalStock = () => {
    /* istanbul ignore next */
    cov_2difmttrrx().f[9]++;
    const warehouseData =
    /* istanbul ignore next */
    (cov_2difmttrrx().s[74]++, form.getValues('warehouseData'));
    /* istanbul ignore next */
    cov_2difmttrrx().s[75]++;
    warehouseData.forEach((warehouse, whIndex) => {
      /* istanbul ignore next */
      cov_2difmttrrx().f[10]++;
      const totalQuantity =
      /* istanbul ignore next */
      (cov_2difmttrrx().s[76]++, warehouse.colors.reduce((total, color) => {
        /* istanbul ignore next */
        cov_2difmttrrx().f[11]++;
