        /* istanbul ignore next */
        cov_2difmttrrx().s[80]++;
        color.inStock = colorTotal > 0;
        /* istanbul ignore next */
        cov_2difmttrrx().s[81]++;
        return total + colorTotal;
      }, 0));
      /* istanbul ignore next */
      cov_2difmttrrx().s[82]++;
      form.setValue(`warehouseData.${whIndex}.quantity`, totalQuantity);
    });
    const totalStock =
    /* istanbul ignore next */
    (cov_2difmttrrx().s[83]++, warehouseData.reduce((sum, wh) => {
      /* istanbul ignore next */
      cov_2difmttrrx().f[13]++;
      cov_2difmttrrx().s[84]++;
      return sum + wh.quantity;
    }, 0));
    /* istanbul ignore next */
    cov_2difmttrrx().s[85]++;
