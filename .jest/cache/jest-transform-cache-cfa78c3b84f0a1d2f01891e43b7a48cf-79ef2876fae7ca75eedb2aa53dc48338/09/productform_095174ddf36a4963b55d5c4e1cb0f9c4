const calculatePricing = (basePrice, listPrice, markup, discount) => {
  /* istanbul ignore next */
  cov_2difmttrrx().f[4]++;
  const commission =
  /* istanbul ignore next */
  (cov_2difmttrrx().s[46]++, basePrice * 0.03) // 3% commission
  ;
  const suggestedPrice =
  /* istanbul ignore next */
  (cov_2difmttrrx().s[47]++, basePrice * (1 + markup / 100));
  const suggestedMarkup =
  /* istanbul ignore next */
  (cov_2difmttrrx().s[48]++, listPrice > basePrice ?
  /* istanbul ignore next */
  (cov_2difmttrrx().b[4][0]++, (listPrice - basePrice) / basePrice * 100) :
  /* istanbul ignore next */
  (cov_2difmttrrx().b[4][1]++, markup));
  let finalPrice =
  /* istanbul ignore next */
  (cov_2difmttrrx().s[49]++,
  /* istanbul ignore next */
  (cov_2difmttrrx().b[5][0]++, listPrice) ||
  /* istanbul ignore next */
  (cov_2difmttrrx().b[5][1]++, suggestedPrice));
  /* istanbul ignore next */
  cov_2difmttrrx().s[50]++;
  if (
  /* istanbul ignore next */
  (cov_2difmttrrx().b[7][0]++, discount) &&
  /* istanbul ignore next */
  (cov_2difmttrrx().b[7][1]++, discount.type !== 'none') &&
  /* istanbul ignore next */
  (cov_2difmttrrx().b[7][2]++, discount.value > 0)) {
    /* istanbul ignore next */
    cov_2difmttrrx().b[6][0]++;
    cov_2difmttrrx().s[51]++;
    if (discount.type === 'percentage') {
      /* istanbul ignore next */
      cov_2difmttrrx().b[8][0]++;
      cov_2difmttrrx().s[52]++;
      finalPrice *= 1 - discount.value / 100;
    } else {
      /* istanbul ignore next */
      cov_2difmttrrx().b[8][1]++;
      cov_2difmttrrx().s[53]++;
      finalPrice -= discount.value;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2difmttrrx().b[6][1]++;
  }
  cov_2difmttrrx().s[54]++;
  finalPrice = Math.max(finalPrice, basePrice);
  const profit =
  /* istanbul ignore next */
  (cov_2difmttrrx().s[55]++, finalPrice - basePrice - commission);
  /* istanbul ignore next */
  cov_2difmttrrx().s[56]++;
  return {
    finalPrice: Number(finalPrice.toFixed(2)),
    profit: Number(profit.toFixed(2)),
    commission: Number(commission.toFixed(2)),
    suggestedMarkup: Number(suggestedMarkup.toFixed(1))
  };
};
