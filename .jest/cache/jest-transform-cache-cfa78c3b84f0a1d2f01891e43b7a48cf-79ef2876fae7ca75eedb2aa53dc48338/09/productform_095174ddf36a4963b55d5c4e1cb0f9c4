    });
  };
  /* istanbul ignore next */
  cov_2difmttrrx().s[67]++;
  const removeColor = colorIndex => {
    /* istanbul ignore next */
    cov_2difmttrrx().f[8]++;
    const currentColors =
    /* istanbul ignore next */
    (cov_2difmttrrx().s[68]++, [...colors]);
    /* istanbul ignore next */
    cov_2difmttrrx().s[69]++;
