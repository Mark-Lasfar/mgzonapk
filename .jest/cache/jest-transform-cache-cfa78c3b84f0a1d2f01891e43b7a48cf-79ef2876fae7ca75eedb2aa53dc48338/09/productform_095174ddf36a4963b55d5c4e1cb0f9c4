    /* istanbul ignore next */
    (cov_2difmttrrx().s[88]++, totalStock === 0 ?
    /* istanbul ignore next */
    (cov_2difmttrrx().b[13][0]++, 'OUT_OF_STOCK') :
    /* istanbul ignore next */
    (cov_2difmttrrx().b[13][1]++, totalStock <= Math.min(...minStocks) ?
    /* istanbul ignore next */
    (cov_2difmttrrx().b[14][0]++, 'LOW_STOCK') :
    /* istanbul ignore next */
    (cov_2difmttrrx().b[14][1]++, 'IN_STOCK')));
    /* istanbul ignore next */
    cov_2difmttrrx().s[89]++;
