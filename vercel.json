{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["fra1"],

  "crons": [
    {
      "path": "/api/cron/check-subscriptions",
      "schedule": "0 0 * * *"
    }
  ],
    "rewrites": [
    {
      "source": "/:storeId/*",
      "destination": "/[locale]/[storeId]/store/[storeId]"
    },
    {
      "source": "/:storeId/:pageSlug",
      "destination": "/[locale]/[storeId]/store/[pageSlug]"
    }
  ]
}